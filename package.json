{
  "name": "url-to-pdf-api",
  "version": "1.0.0",
  "description": "Web page PDF rendering done right. Packaged to an easy API.",
  "main": "src/index.js",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "startnodaemon": "pm2-runtime start ecosystem.config.js --env production",
    "develop": "nodemon --watch ./src -e js src/index.js",
    "test": "mocha --timeout 10000 && npm run lint",
    "lint": "eslint .",
    "pm2": "node_modules/.bin/pm2",
    "start": "node_modules/.bin/pm2 start ecosystem.config.js --env production",
    "restart": "node_modules/.bin/pm2 restart url-to-pdf --update-env",
    "reload": "node_modules/.bin/pm2 reload ecosystem.config.js",
    "stop": "node_modules/.bin/pm2 stop url-to-pdf",
    "startdev": "node_modules/.bin/pm2 start ecosystem.config.js",
    "flush": "node_modules/.bin/pm2 flush"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvarcarto/url-to-pdf-api.git"
  },
  "author": "Kimmo Brunfeldt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alvarcarto/url-to-pdf-api/issues"
  },
  "homepage": "https://github.com/alvarcarto/url-to-pdf-api#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.15.5",
    "express-validation": "^1.0.2",
    "generic-pool": "^3.7.1",
    "joi": "^11.1.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.1",
    "pdf-parse": "^1.1.1",
    "pm2": "^3.5.0",
    "puppeteer": "^1.14.0",
    "server-destroy": "^1.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  }
}
